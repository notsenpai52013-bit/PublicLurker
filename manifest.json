{
  "name": "Lurker V3",
  "version": "7.5.3",
  "description": "Lurker V3 released by Not_Senpai for Miniblox",
  "manifest_version": 3,
  "permissions": ["declarativeNetRequest", "scripting", "tabs"],
  "host_permissions": ["https://miniblox.io/*"],
  "declarative_net_request": {"rule_resources": [{"id": "ruleset_1", "enabled": true, "path": "rules.json"}]},
	"content_scripts": [{"matches": ["https://miniblox.io/*"], "js": ["content.js"], "run_at": "document_start"}],
	"web_accessible_resources": [{"resources": ["publiclurker.js"], "matches": ["<all_urls>"]}],
	"icons": {"16": "hello3.png", "48": "hello3.png", "128": "hello3.png"}
}

